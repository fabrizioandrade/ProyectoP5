{
  "source": {
    "include": ["./"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "recurse": true,
    "destination": "./docs/",
    "readme": "../README.md"
  }
}
